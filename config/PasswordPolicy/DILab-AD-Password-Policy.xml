<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE PasswordPolicy PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<PasswordPolicy description="" name="DILab-AD-Password-Policy">
  <PasswordConstraints>
    <Map>
      <entry key="checkCaseSensitive">
        <value>
          <Boolean/>
        </value>
      </entry>
      <entry key="checkPasswordAgainstAccountID">
        <value>
          <Boolean/>
        </value>
      </entry>
      <entry key="checkPasswordAgainstDisplayName">
        <value>
          <Boolean/>
        </value>
      </entry>
      <entry key="checkPasswordTriviality">
        <value>
          <Boolean/>
        </value>
      </entry>
      <entry key="checkPasswordsAgainstAccountAttributes">
        <value>
          <Boolean/>
        </value>
      </entry>
      <entry key="checkPasswordsAgainstDictionary">
        <value>
          <Boolean/>
        </value>
      </entry>
      <entry key="checkPasswordsAgainstIdentityAttributes">
        <value>
          <Boolean/>
        </value>
      </entry>
      <entry key="minAccountIDUniqueChars"/>
      <entry key="minDisplayNameUniqueChars"/>
      <entry key="minHistoryUniqueChars"/>
      <entry key="passwordHistory" value="6"/>
      <entry key="passwordMaxLength"/>
      <entry key="passwordMinAlpha" value="8"/>
      <entry key="passwordMinCharType" value="3"/>
      <entry key="passwordMinLength"/>
      <entry key="passwordMinLower" value="1"/>
      <entry key="passwordMinNumeric" value="1"/>
      <entry key="passwordMinSpecial" value="1"/>
      <entry key="passwordMinUpper" value="1"/>
      <entry key="passwordRepeatedChar"/>
    </Map>
  </PasswordConstraints>
</PasswordPolicy>